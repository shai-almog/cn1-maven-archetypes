<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="filechooser"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <modules>

    <module id="${rootArtifactId}-common" dir="common" name="${rootArtifactId}-common">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/css</directory>
          <includes>
            <include>**/*.txt</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/css</directory>
          <includes>
            <include>**/*.css</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/test/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>codenameone_library_required.properties</include>
            <include>codenameone_library_appended.properties</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-javase" dir="javase" name="${rootArtifactId}-javase">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-javascript" dir="javascript" name="${rootArtifactId}-javascript">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/javascript</directory>
          <includes>
            <include>**/*.js</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-ios" dir="ios" name="${rootArtifactId}-ios">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/main/objectivec</directory>
          <includes>
            <include>**/*.h</include>
            <include>**/*.m</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-win" dir="win" name="${rootArtifactId}-win">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/csharp</directory>
          <includes>
            <include>**/*.cs</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-android" dir="android" name="${rootArtifactId}-android">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-lib" dir="lib" name="${rootArtifactId}-lib" />
    <module id="${rootArtifactId}-tests" dir="tests" name="${rootArtifactId}-tests">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>.mvn</directory>
          <includes>
            <include>**/*.config</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>cn1libs</directory>
          <includes>
            <include>**</include>
          </includes>
        </fileSet>
      </fileSets>
      <modules>
        <module id="${rootArtifactId}-tests-common" dir="common" name="${rootArtifactId}-tests-common">
          <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
              <directory>src/main/java</directory>
              <includes>
                <include>**/*.java</include>
              </includes>
            </fileSet>
            <fileSet encoding="UTF-8">
              <directory>src/main/css</directory>
              <includes>
                <include>**/*.css</include>
              </includes>
            </fileSet>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
              <directory>src/test/java</directory>
              <includes>
                <include>**/*.java</include>
              </includes>
            </fileSet>
            <fileSet filtered="true" encoding="UTF-8">
              <directory></directory>
              <includes>
                <include>codenameone_settings.properties</include>
                <include>nbactions.xml</include>
              </includes>
            </fileSet>
          </fileSets>
        </module>
        <module id="${rootArtifactId}-tests-javase" dir="javase" name="${rootArtifactId}-tests-javase" />
      </modules>
    </module>
  </modules>
</archetype-descriptor>
